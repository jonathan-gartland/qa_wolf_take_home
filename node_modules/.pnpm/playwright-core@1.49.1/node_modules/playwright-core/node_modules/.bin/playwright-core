#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jgartland/Projects/Node/node-22/qa_wolf_take_home/node_modules/.pnpm/playwright-core@1.49.1/node_modules/playwright-core/node_modules:/Users/jgartland/Projects/Node/node-22/qa_wolf_take_home/node_modules/.pnpm/playwright-core@1.49.1/node_modules:/Users/jgartland/Projects/Node/node-22/qa_wolf_take_home/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jgartland/Projects/Node/node-22/qa_wolf_take_home/node_modules/.pnpm/playwright-core@1.49.1/node_modules/playwright-core/node_modules:/Users/jgartland/Projects/Node/node-22/qa_wolf_take_home/node_modules/.pnpm/playwright-core@1.49.1/node_modules:/Users/jgartland/Projects/Node/node-22/qa_wolf_take_home/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
